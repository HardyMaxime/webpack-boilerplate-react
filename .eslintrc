{
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "extends": "standard",
    "rules": {
        "arrow-parens": 0,
        "generator-star-spacing": 0,
        "no-debugger": 0,
        "no-new": 0,
        "comma-dangle": ["error", "never"],
        "indent": 0,
        "no-unused-vars": 0
    }
}